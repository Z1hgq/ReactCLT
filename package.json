{
  "name": "react-clt",
  "version": "1.0.0",
  "description": "React Components Library Template",
  "scripts": {
    "start": "dumi dev",
    "release": "npm run compile && dumi build && node build-site.js",
    "compile": "node scripts/cli/run.js compile",
    "compile:less": "node scripts/cli/run.js compile:less",
    "build": "npm run compile",
    "dist": "node scripts/cli/run.js dist",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "lint-staged && npm run release"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "es"
  ],
  "license": "MIT",
  "author": "Z1hgq",
  "keywords": [
    "component",
    "components",
    "frontend",
    "react",
    "react-components",
    "template"
  ],
  "siteOutputName": "react-clt",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "resolutions": {
    "ajv": "@6.5.5"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {},
